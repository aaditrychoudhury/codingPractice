
import java.util.Scanner;


public class Main {
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int N = s.nextInt();
		int i = 1, temp = 1;
		if (N == 0 || N == 1) {
			System.out.println(N);
		}else {
			while(temp <= N) {
				++i;
				temp = i*i;
				}
			System.out.println(i - 1);
		}


	}
}
