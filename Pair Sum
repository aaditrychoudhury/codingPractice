public class Solution {  

    public static int pairSum(int arr[], int x) {
    	//Your code goes here
        int j;
        int i;
    	int count = 0;
        for(i = 0;i<arr.length;i++){
            // int temp = arr[i];
            for(j = i+1;j<arr.length;j++){
                
                if(arr[i]+arr[j]==x)
                	count++;
                  
                }
            
        }
        return count;
    }
}
