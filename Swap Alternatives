
public class Solution {
    
    public static void swapAlternate(int arr[]) {
    	//Your code goes here
        int temp;
        int size = arr.length;
        int mod = size%2;
        size = size - mod;
        for(int i =0;i<size;i+=2){
            temp = arr[i];
            arr[i]=arr[i+1];
            arr[i+1] = temp;
        }
    }
}
